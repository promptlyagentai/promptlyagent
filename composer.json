{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/livewire-starter-kit",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/promptlyagentai/notion-integration.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/promptlyagentai/http-webhook-integration.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/promptlyagentai/slack-integration.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/promptlyagentai/schedule-trigger.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/promptlyagentai/perplexity-integration.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/promptlyagentai/email-output-action.git"
        }
    ],
    "type": "project",
    "description": "AI-powered research and knowledge management platform. Laravel 12 + Livewire 3 + Volt + Flux UI with multi-agent orchestration, RAG pipelines, and real-time streaming.",
    "keywords": ["laravel", "framework", "tall-stack", "flux-ui", "livewire", "volt", "ai", "rag", "multi-agent"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "fakerphp/faker": "^1.24",
        "filament/filament": "^3.0",
        "fivefilters/readability.php": "^1.0",
        "guzzlehttp/guzzle": "^7.9",
        "knuckleswtf/scribe": "^5.6",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.33",
        "laravel/reverb": "^1.0",
        "laravel/sanctum": "^4.2",
        "laravel/scout": "^10.17",
        "laravel/socialite": "^5.21",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/html-to-markdown": "^5.1",
        "livewire/flux": "^2.1.1",
        "livewire/volt": "^1.7.0",
        "meilisearch/meilisearch-php": "^1.15",
        "opgginc/laravel-mcp-server": "^1.3",
        "phpoffice/phpword": "^1.4",
        "predis/predis": "^2.0",
        "prism-php/bedrock": "^1.6",
        "prism-php/prism": "^0.90.0",
        "prism-php/relay": "^1.1",
        "promptlyagentai/email-output-action": "@dev",
        "promptlyagentai/http-webhook-integration": "@dev",
        "promptlyagentai/notion-integration": "@dev",
        "promptlyagentai/perplexity-integration": "@dev",
        "promptlyagentai/schedule-trigger": "@dev",
        "promptlyagentai/slack-integration": "@dev",
        "pusher/pusher-php-server": "^7.2",
        "resend/resend-php": "^1.1",
        "smalot/pdfparser": "^2.12",
        "symfony/expression-language": "^8.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "cweagans/composer-patches": "^2.0",
        "hyvor/laravel-playwright": "^2.0",
        "kitloong/laravel-migrations-generator": "^7.2",
        "laravel/boost": "^2.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.45",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "docs": [
            "@php -d error_reporting='E_ALL & ~E_DEPRECATED' artisan scribe:generate --no-interaction"
        ],
        "docs:force": [
            "@php -d error_reporting='E_ALL & ~E_DEPRECATED' artisan scribe:generate --force --no-interaction"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-debugbar"
            ]
        },
        "patches": {
            "prism-php/relay": [
                "patches/prism-relay-named-arguments.patch"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "cweagans/composer-patches": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
